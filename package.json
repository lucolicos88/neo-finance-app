{
  "name": "neoformula-finance-app",
  "version": "1.0.0",
  "description": "Aplicativo de gestão financeira e contábil em Google Apps Script",
  "main": "dist/main.js",
  "scripts": {
    "build": "webpack",
    "watch": "webpack --watch",
    "push": "clasp push",
    "deploy": "npm run build && clasp push",
    "open": "clasp open",
    "logs": "clasp logs",
    "setup": "node setup-spreadsheet.js",
    "setup-bound": "powershell -ExecutionPolicy Bypass -File ./setup-bound.ps1",
    "bind": "node bind-to-spreadsheet.js",
    "setup-simples": "powershell -ExecutionPolicy Bypass -File ./setup-manual-simples.ps1",
    "lint": "eslint \"src/**/*.{ts,js}\"",
    "format": "prettier --write \"src/**/*.{ts,js,json,html,css}\""
  },
  "keywords": [
    "google-apps-script",
    "finance",
    "accounting",
    "neoformula"
  ],
  "author": "Neoformula",
  "license": "UNLICENSED",
  "devDependencies": {
    "@google/clasp": "^2.4.2",
    "@types/google-apps-script": "^1.0.83",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "copy-webpack-plugin": "^13.0.1",
    "gas-webpack-plugin": "^2.6.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "ts-loader": "^9.5.4",
    "typescript": "^5.3.3",
    "webpack": "^5.103.0",
    "webpack-cli": "^6.0.1"
  }
}
